        <li>
            <div class="onekey-credit">
                {% if shop.recomended > 0 %}
                <div class="onekey-credit-recommend">{%trans%}Recomended{%endtrans%}</div>
                {% endif %}
                <a class="onekey-credit-img" href="{{ path('shop', {'serviceslug': shop.slug}) }}">
                    <img src="/img/icons/{{shop.img}}" height="99" width="300" alt="">
                    <span>{{shop.company}}</span>
                </a>
                <div class="onekey-credit-inner">
                    <div class="onekey-credit-info clearfix">
                        <div class="onekey-credit-cell">
                            <div class="onekey-credit-label">{%trans%}To payoff{%endtrans%}</div>
                            <div class="onekey-credit-value total"><span>
                                {% if shop.roundtype == 1 %}
                                   {{ (shop.commision + money)|round }}
                                {% elseif shop.roundtype == 2 %}
                                   {{ (shop.commision + money)|round(0,'ceil') }}
                                {% elseif shop.roundtype == 3 %}
                                   {{ (shop.commision + money)|round(0,'floor') }}
                                {% elseif shop.roundtype == 4 %}
                                   {{ (shop.commision + money)|round(1) }}
                                {% else %}
                                   {{ (shop.commision + money)|round(2) }}                            
                                {% endif %}   
                            </span>грн</div>
                        </div>
                        <div class="onekey-credit-cell">
                            <div class="onekey-credit-label">{%trans%}Body of credit{%endtrans%}</div>
                            <div class="onekey-credit-value"><span>{{money}}</span>грн</div>
                        </div>
                        <div class="onekey-credit-cell">
                            <div class="onekey-credit-label">{%trans%}Commission{%endtrans%}</div>
                            <div class="onekey-credit-value"><span>
                                {% if shop.roundtype == 1 %}
                                   {{ (shop.commision)|round }}
                                {% elseif shop.roundtype == 2 %}
                                   {{ (shop.commision)|round(0,'ceil') }}
                                {% elseif shop.roundtype == 3 %}
                                   {{ (shop.commision)|round(0,'floor') }}
                                {% elseif shop.roundtype == 4 %}
                                   {{ (shop.commision)|round(1) }}
                                {% else %}
                                   {{ (shop.commision)|round(2) }}                            
                                {% endif %}   
                            </span>грн</div>
                        </div>              
                    </div>
                    <div class="onekey-credit-info small clearfix">
                        <div class="onekey-credit-cell">
                            <div class="onekey-credit-label"><a href="{{ path('shop', {'serviceslug': shop.slug}) }}#review">Смотреть все отзывы&nbsp;({{shop.commentscount}})</a></div>
                        </div>
                        <div class="onekey-credit-cell">
                            <div class="onekey-credit-label">Рейтинг</div>
                            <div class="onekey-reply-stars"><span>
                                {% for num in 1 .. 5 %}
                                    <i class="{%if num <= shop.avgrank %}fa fa-star{%else%}fa fa-star-o{%endif%}" style="font-size: 16px" aria-hidden="true"></i>
                                {% endfor %}
                            </span></div>
                        </div>
                    </div>                                
                </div>
                <a class="onekey-btn" href="{{shop.landing}}">{%trans%}Make a request{%endtrans%}</a>
            </div>
        </li>
